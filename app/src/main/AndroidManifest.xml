<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodorderapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".JapaneseFoods"></activity>
        <activity android:name=".ChineseFoods" />
        <activity android:name=".ItalianFoods" />
        <activity android:name=".FinalCartPreview" />
        <activity android:name=".MexicanFoods" />
        <activity android:name=".IndianFoods" />
        <activity android:name=".AmericanFoods" />
        <activity android:name=".HomeDashBoard"/>
        <activity android:name=".ProfileToggle" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignUpActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewDataBase" />
    </application>

</manifest>